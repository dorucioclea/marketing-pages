{% from 'components/label/macro.njk' import label %}

{% macro previewQuestionDataText(questionData) %}
  {% if questionData and questionData.trim().length > 0 %}
    <div data-test-id="preview-question-data-text">
      {{ label({
        "text": questionData
      }) }}
    </div>
  {% endif %}
{% endmacro %}